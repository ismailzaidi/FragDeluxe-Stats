<?xml version="1.0" encoding="utf-8"?>
<com.fragdeluxestats.customviews.PlayerOnlineView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:orientation="horizontal">

    <com.fragdeluxestats.customviews.TextStyleView
        android:id="@+id/playerCounter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:text="1"
        android:textSize="10sp"
        app:textType="normal" />

    <com.fragdeluxestats.customviews.TextStyleView
        android:id="@+id/playerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textColor="@android:color/white"
        android:lines="2"
        android:textSize="8sp"
        android:text="Norsken"
        app:textType="bold" />
</com.fragdeluxestats.customviews.PlayerOnlineView>